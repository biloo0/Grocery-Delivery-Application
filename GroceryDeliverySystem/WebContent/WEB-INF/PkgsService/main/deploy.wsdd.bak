<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from PkgsService WSDL service -->

  <service name="Pkgs" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://main"/>
      <parameter name="wsdlServiceElement" value="PkgsService"/>
      <parameter name="schemaQualified" value="http://main"/>
      <parameter name="wsdlServicePort" value="Pkgs"/>
      <parameter name="className" value="main.PkgsSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Pkgs"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getPkgs" qname="operNS:getPkgs" xmlns:operNS="http://main" returnQName="retNS:getPkgsReturn" xmlns:retNS="http://main" returnType="rtns:PackDetails" xmlns:rtns="http://main" soapAction="" >
        <parameter qname="pns:product" xmlns:pns="http://main" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getPkgs"/>

      <typeMapping
        xmlns:ns="http://main"
        qname="ns:PackDetails"
        type="java:main.PackDetails"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
